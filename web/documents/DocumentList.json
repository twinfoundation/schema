{
	"type": "object",
	"properties": {
		"@context": {
			"type": "array",
			"minItems": 2,
			"items": [
				{
					"type": "string",
					"const": "https://schema.twindev.org/documents/"
				},
				{
					"type": "string",
					"const": "https://schema.twindev.org/common/"
				}
			],
			"additionalItems": {
				"$ref": "https://schema.twindev.org/json-ld/JsonLdContextDefinitionElement"
			},
			"description": "JSON-LD Context."
		},
		"type": {
			"type": "string",
			"const": "DocumentList",
			"description": "JSON-LD Type."
		},
		"documents": {
			"type": "array",
			"items": {
				"$ref": "https://schema.twindev.org/documents/Document"
			},
			"description": "The list of documents."
		},
		"edges": {
			"type": "array",
			"items": {
				"type": "string"
			},
			"description": "The ids of the other vertices which are connected to the document."
		},
		"cursor": {
			"type": "string",
			"description": "The cursor to get the next chunk of documents."
		}
	},
	"required": ["@context", "type", "documents"],
	"additionalProperties": false,
	"description": "Interface describing a list of document entries."
}
