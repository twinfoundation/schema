{
	"@context": {
		"@version": 1.1,
		"@protected": true,
		"twin-immutable-proof": "https://schema.twindev.org/immutable-proof/",
		"id": "@id",
		"type": "@type",
		"ImmutableProof": "https://schema.twindev.org/immutable-proof/ImmutableProof",
		"ImmutableProofVerification": "https://schema.twindev.org/immutable-proof/ImmutableProofVerification",
		"proofObjectId": {
			"@id": "twin-immutable-proof:proofObjectId",
			"@type": "schema:Text"
		},
		"proofObjectHash": {
			"@id": "twin-immutable-proof:proofObjectHash",
			"@type": "schema:Text"
		},
		"proof": {
			"@id": "https://w3id.org/security#proof",
			"@type": "@id",
			"@container": "@graph"
		},
		"immutableReceipt": "twin-immutable-proof:immutableReceipt",
		"verified": {
			"@id": "twin-immutable-proof:verified",
			"@type": "schema:Boolean"
		},
		"failure": {
			"@id": "twin-immutable-proof:failure",
			"@type": "schema:Text"
		}
	}
}