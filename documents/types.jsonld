{
	"@context": {
		"@version": 1.1,
		"@protected": true,
		"twin-documents": "https://schema.twindev.org/documents/",
		"twin-blob-storage": "https://schema.twindev.org/blob-storage/",
		"twin-attestation": "https://schema.twindev.org/attestation/",
		"schema": "http://schema.org/",
		"id": "@id",
		"type": "@type",
		"Document": "twin-documents:Document",
		"DocumentList": "twin-documents:DocumentList",
		"documentIdFormat": {
			"@id": "twin-documents:documentIdFormat",
			"@type": "schema:Text"
		},
		"documentCode": {
			"@id": "twin-documents:documentCode",
			"@type": "schema:identifier"
		},
		"documentRevision": {
			"@id": "twin-documents:documentRevision",
			"@type": "schema:Integer"
		},
		"annotationObject": {
			"@id": "twin-documents:annotationObject",
			"@type": "@json"
		},
		"blobStorageId": {
			"@id": "twin-documents:blobStorageId",
			"@type": "schema:identifier"
		},
		"blobHash": {
			"@id": "twin-documents:blobHash",
			"@type": "schema:Text"
		},
		"blobStorageObject": {
			"@id": "twin-documents:blobStorageObject",
			"@type": "twin-blob-storage:BlobStorageEntry"
		},
		"attestationId": {
			"@id": "twin-documents:attestationId",
			"@type": "schema:identifier"
		},
		"attestationObject": {
			"@id": "twin-documents:attestationObject",
			"@type": "twin-attestation:AttestationInformation"
		},
		"documents": {
			"@id": "twin-documents:documents",
			"@container": "@list"
		}
	}
}